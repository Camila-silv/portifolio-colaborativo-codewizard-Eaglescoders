{
  "version": 2,
  "builds": [
    {
      "src": "api/index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/index.js"
    }
  ],
  "env": {
    "MONGO_URI": "@mongo_uri",
    "PORT": "5000", 
    "URL_ORIGIN": "https://portifolio-colaborativo-codewizard-eaglescoders.vercel.app"
  }
}
